@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Lista de livros</h1>
</div>

<div id="livros"></div>

@section Scripts{
    <script>
        $(function () {
            $.get("/Livro/_List", function (data) {
                $("#livros").html(data);
            })
        })
    </script>
}